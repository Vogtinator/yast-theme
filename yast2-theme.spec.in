@HEADER-COMMENT@
# neededforbuild yast2-devel-packages

@HEADER@
BuildArchitectures: noarch
Summary:	-
%description
-

%package SuSELinux

Summary:	-
Version:	@VERSION@
Group:		System/YaST
Provides:	yast2_theme
PreReq:		/bin/ln

%description SuSELinux
-


@PREP@

@BUILD@

@INSTALL@
cp -R "$RPM_BUILD_ROOT/@docdir@" "$RPM_BUILD_ROOT/@docdir@-SuSELinux"
rm -rf "$RPM_BUILD_ROOT/@docdir@"

@CLEAN@

%post SuSELinux
cd @themedir@
ln -sf SuSELinux current

%files SuSELinux
%defattr(-,root,root)
%dir @themedir@
%dir @themedir@/SuSELinux
@themedir@/SuSELinux/*
%dir @themedir@/SuSELinux/wizard
@themedir@/SuSELinux/wizard/*
%dir @themedir@/SuSELinux/testpage
@themedir@/SuSELinux/testpage/*
%doc @docdir@-SuSELinux
