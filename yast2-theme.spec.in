@HEADER-COMMENT@

# neededforbuild  yast2-devtools

@HEADER@
BuildArchitectures: noarch
Summary:	-
%description
-

%package SuSELinux

Summary:	-
Group:		System/YaST
Provides:	yast2_theme
PreReq:		/bin/rm /bin/ln

%description SuSELinux
-

%package UnitedLinux

Summary:	-
Group:		System/YaST
Provides:	yast2_theme
PreReq:		/bin/rm /bin/ln

%description UnitedLinux
-

@PREP@

@BUILD@

@INSTALL@

@CLEAN@

%post SuSELinux
cd @themedir@
rm -f current
ln -sf SuSELinux current

%post UnitedLinux
cd @themedir@
rm -f current
ln -sf UnitedLinux current

%files SuSELinux
%defattr(-,root,root)
%dir @themedir@
%dir @themedir@/SuSELinux
@themedir@/SuSELinux/*

%files UnitedLinux
%defattr(-,root,root)
%dir @themedir@
%dir @themedir@/UnitedLinux
@themedir@/UnitedLinux/*
